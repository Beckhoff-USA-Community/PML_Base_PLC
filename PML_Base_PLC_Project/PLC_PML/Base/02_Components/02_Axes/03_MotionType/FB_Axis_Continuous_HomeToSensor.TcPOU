    <Method Name="Monitoring" Id="{a8ec1f3d-a675-4e0e-86be-94c439cdb928}" FolderPath="01_SUPER_Overrides\">
      <Declaration><![CDATA[METHOD Monitoring : BOOL]]></Declaration>
      <Implementation>
        <ST><![CDATA[IF fbMoveModulo.Busy THEN
	bActive := TRUE;
END_IF

fModuloPosition := SelectedAxis.NcToPlc.ModuloActPos;
fModuloSetpoint := SelectedAxis.NcToPlc.ModuloSetPos;

IF fbWriteParameter.Done AND fbWriteParameter.ParameterNumber = MC_AxisParameter.AxisEncoderModuloValue THEN
	fModuloFactor := fbWriteParameter.Value;
END_IF

IF fbWriteParameter.Error AND NOT a_fbAxisBaseAlarms[E_AxisBase.WriteParameterError].bRaised THEN
	RaiseAlarmWithStrings(	fbAlarm :=  a_fbAxisBaseAlarms[E_AxisBase.WriteParameterError], UDINT_TO_STRING(fbWriteParameter.ErrorID), '');
										
ELSIF NOT fbAxisPower.Error AND a_fbAxisBaseAlarms[E_AxisBase.WriteParameterError].bRaised THEN
	a_fbAxisBaseAlarms[E_AxisBase.WriteParameterError].Clear(0,0);
END_IF

SUPER^.Monitoring();		//Called last to allow the bReady bit to be evaluated after all error checking is done

eCurrentAlarmSeverity := F_GetMaxSeverityRaised(	a_fbAlarms := a_fbAxisBaseAlarms,
													eCurrentSeverity := eCurrentAlarmSeverity);]]></ST>
      </Implementation>
    </Method>
    <Method Name="MotionControl" Id="{5ed14d51-d145-4b12-b9b6-42755a0e8a41}" FolderPath="01_SUPER_Overrides\">
      <Declaration><![CDATA[METHOD MotionControl : BOOL
VAR_INPUT
END_VAR
]]></Declaration>
      <Implementation>
        <ST><![CDATA[SUPER^.MotionControl();

fbMoveModulo(
	Axis:= SelectedAxis,
	Velocity:= fTargetVelocity, 
	Direction:= MC_Direction.MC_Positive_Direction);
	
fbMoveModulo.Execute := FALSE;]]></ST>
      </Implementation>
    </Method>
    <Method Name="MoveModulo" Id="{93fcfbfa-3e20-494f-9bfa-e9c0ba2c3d7c}" FolderPath="02_ITF_Axis_Continuous\">
      <Declaration><![CDATA[METHOD MoveModulo : BOOL
VAR_INPUT
	override 	: BOOL := FALSE;		//Allows this instruction to override another ongoing operation(This parameter is initialized so that once default parameter value functionality is added to TwinCAT, this input will not need to be set explicitly in the Method call)
	position	: LREAL;				//Desired position to which the axis should move
END_VAR]]></Declaration>
      <Implementation>
        <ST><![CDATA[IF override OR NOT bActive AND NOT bError THEN
	
	fbMoveModulo.Position := position;
	fbMoveModulo.Execute := TRUE;
	
END_IF]]></ST>
      </Implementation>
    </Method>
    <Method Name="SetAxisModuloFactor" Id="{109fb69d-17a5-4472-8362-4f4899d1ee1b}" FolderPath="02_ITF_Axis_Continuous\">
      <Declaration><![CDATA[METHOD SetAxisModuloFactor : BOOL
VAR_INPUT
	fSetAxisModuloFactor		: LREAL;
END_VAR
]]></Declaration>
      <Implementation>
        <ST><![CDATA[IF NOT bActive THEN
	IF NOT fbWriteParameter.Busy THEN
	fbWriteParameter(
						Axis:= SelectedAxis, 
						Execute:= TRUE, 
						ParameterNumber:= MC_AxisParameter.AxisEncoderModuloValue, 
						Value:= fSetAxisModuloFactor, 
						Done=> , 
						Busy=> , 
						Error=> , 
						ErrorID=> );
	END_IF
ELSE
	//TODO: Messaging for "Axis not ready for parameter write"
END_IF]]></ST>
      </Implementation>
    </Method>
